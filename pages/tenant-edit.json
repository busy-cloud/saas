{
  "title": "编辑租户",
  "template": "form",
  "toolbar": [
    {
      "type": "button",
      "label": "选择父级ID",
      "action": {
        "type": "dialog",
        "page": "saas/tenant-choose",
        "after_close": "this.editor.patchValue({parent_id: result.id})"
      }
    }
  ],
  "fields": [
    {
      "key": "id",
      "label": "ID",
      "type": "text",
      "required": true
    },
    {
      "key": "name",
      "label": "名称",
      "type": "text"
    },
    {
      "key": "parent_id",
      "label": "父级ID",
      "type": "text",
      "required": true
    },
    {
      "key": "disabled",
      "label": "禁用",
      "type": "switch"
    }
  ],
  "load_api": "table/tenant/detail/:id",
  "submit_api": "table/tenant/update/:id",
  "submit_success": "this.navigate('/page/saas/tenant')",
  "methods": {
  }
}